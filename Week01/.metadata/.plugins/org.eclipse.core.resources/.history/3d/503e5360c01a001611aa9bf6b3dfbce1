
public class Program {

	public static boolean isOdd(int n){
		return (n % 2) == 1;
	}
	
	public static boolean isPrime(int n){
		
		if(n < 2)
			return false;
		
		for (int i = 2; i <= Math.sqrt(n); i++) {
			if(n % i == 0)
				return false;
		}
	}
	
	public static void main(String[] args) {

		
	}

}
